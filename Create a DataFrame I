import codecademylib3
import pandas as pd

# Creating the DataFrame with Product ID, Product Name, and Color
df1 = pd.DataFrame({
  'Product ID': [1, 2, 3, 4],                   # Product IDs
  'Product Name': ['t-shirt', 't-shirt', 'skirt', 'skirt'],  # Product Names
  'Color': ['blue', 'green', 'red', 'black']   # Colors
})

print(df1)
