# Step 1: Import codecademylib3_seaborn
import codecademylib3_seaborn

# Step 1: Import pyplot from matplotlib with alias plt
from matplotlib import pyplot as plt

# Step 2: Import random
import random

# Step 3: Create numbers_a
numbers_a = range(1, 13)

# Step 4: Create numbers_b
numbers_b = random.sample(range(1000), 12)

# Optional: Print numbers_b to see the random sample
print(numbers_b)

# Step 5: Plot numbers_a against numbers_b
plt.plot(numbers_a, numbers_b)

# Step 6: Show the plot
plt.show()
